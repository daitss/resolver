<resolution time="<%= Time.now.xmlschema %>" outcome="<%= @outcome %>" file_name="<%= file_url %>">
  <% @broken_links.each do |link, ext| %>
          <schema status="unresolved" url="<%= link %>" type="<%= ext[0] %>" message="<%= ext[1] %>" />
  <% end %>
  <% @redirect.each do |link, to_link| %>
          <schema status="redirected" url="<%= link %>" redirect_url="<%= to_link %>" />
  <% end %>
  <% @checksum.each do |link, digest| %>
          <schema status="successful" url="<%= link %>" checksum="<%= digest %>" />
  <% end %>
</resolution>
