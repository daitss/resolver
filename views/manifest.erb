<resolution outcome="<%= @outcome %>" file_name="<%= file_url %>" time="<%= Time.now.xmlschema %>">
  <% @broken_links.each do |link, ext| %>
          <resource status="broken" url="<%= link %>" type="<%= ext[0] %>" message="<%= ext[1] %>" />
  <% end %>
  <% @redirect.each do |link, to_link| %>
          <resource status="redirected" url="<%= link %>" redirect_url="<%= to_link %>" />
  <% end %>
  <% @checksum.each do |link, digest| %>
          <resource status="successful" url="<%= link %>" checksum="<%= digest %>" />
  <% end %>
</resolution>
